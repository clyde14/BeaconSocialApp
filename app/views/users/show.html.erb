<% provide(:title, "View User Profile") %>
<h1>View User Profile</h1>
<p id="notice"><%= notice %></p>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>Nickname:</strong>
  <%= @user.nickname %>
</p>

<% if logged_in? %>
	<%= link_to 'Edit', edit_user_path(@user) %>
<% end %>
